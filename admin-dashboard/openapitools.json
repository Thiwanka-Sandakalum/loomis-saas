{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.20.0"
  },
  "generators": {
    "typescript-angular": {
      "inputSpec": "openapi.yaml",
      "output": "src/app/core/api-client/",
      "generatorName": "typescript-angular",
      "additionalProperties": {
        "ngVersion": "21.1.0",
        "providedInRoot": true,
        "fileNaming": "kebab-case"
      }
    }
  }
}